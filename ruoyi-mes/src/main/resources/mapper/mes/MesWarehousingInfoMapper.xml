    <update id="updateMesWarehousingInfo" parameterType="MesWarehousingInfo">
        update mes_warehousing_info
        <trim prefix="SET" suffixOverrides=",">
            <if test="crystalCode != null">crystal_code = #{crystalCode},</if>
            <if test="spec != null">spec = #{spec},</if>
            <if test="type != null">type = #{type},</if>
            <if test="length != null">length = #{length},</if>
            <if test="thicknessSplice != null">thickness_splice = #{thicknessSplice},</if>
            <if test="slicerModel != null">slicer_model = #{slicerModel},</if>
            <if test="storageLocation != null">storage_location = #{storageLocation},</if>
            <if test="warehousingTime != null">warehousing_time = #{warehousingTime},</if>
        </trim>
