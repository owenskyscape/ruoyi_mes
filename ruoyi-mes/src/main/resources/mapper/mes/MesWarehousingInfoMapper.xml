            <if test="spec != null">spec,</if>
            <if test="type != null">type,</if>
            <if test="length != null">length,</if>
            <if test="thicknessSplice != null">thickness_splice,</if>
            <if test="slicerModel != null">slicer_model,</if>
            <if test="storageLocation != null">storage_location,</if>
            <if test="warehousingTime != null">warehousing_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="crystalCode != null">#{crystalCode},</if>
            <if test="spec != null">#{spec},</if>
            <if test="type != null">#{type},</if>
            <if test="length != null">#{length},</if>
            <if test="thicknessSplice != null">#{thicknessSplice},</if>
            <if test="slicerModel != null">#{slicerModel},</if>
            <if test="storageLocation != null">#{storageLocation},</if>
            <if test="warehousingTime != null">#{warehousingTime},</if>
         </trim>
