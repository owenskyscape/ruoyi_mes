    <select id="selectMesWarehousingInfoList" parameterType="MesWarehousingInfo" resultMap="MesWarehousingInfoResult">
        <include refid="selectMesWarehousingInfoVo"/>
        <where>  
            <if test="crystalCode != null  and crystalCode != ''"> and crystal_code = #{crystalCode}</if>
            <if test="spec != null  and spec != ''"> and spec = #{spec}</if>
            <if test="type != null  and type != ''"> and type = #{type}</if>
            <if test="length != null "> and length = #{length}</if>
            <if test="thicknessSplice != null  and thicknessSplice != ''"> and thickness_splice = #{thicknessSplice}</if>
            <if test="slicerModel != null  and slicerModel != ''"> and slicer_model = #{slicerModel}</if>
            <if test="storageLocation != null  and storageLocation != ''"> and storage_location = #{storageLocation}</if>
            <if test="warehousingTime != null "> and warehousing_time = #{warehousingTime}</if>
        </where>
    </select>
    
    <select id="selectMesWarehousingInfoById" parameterType="Long" resultMap="MesWarehousingInfoResult">
        <include refid="selectMesWarehousingInfoVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertMesWarehousingInfo" parameterType="MesWarehousingInfo" useGeneratedKeys="true" keyProperty="id">
        insert into mes_warehousing_info
        <trim prefix="(" suffix=")" suffixOverrides=",">
