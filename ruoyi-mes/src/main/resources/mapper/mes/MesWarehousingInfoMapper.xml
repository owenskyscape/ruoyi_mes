    </update>

    <delete id="deleteMesWarehousingInfoById" parameterType="Long">
        delete from mes_warehousing_info where id = #{id}
    </delete>

    <delete id="deleteMesWarehousingInfoByIds" parameterType="String">
        delete from mes_warehousing_info where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

