<mapper namespace="com.ruoyi.mes.mapper.MesWarehousingInfoMapper">
    
    <resultMap type="MesWarehousingInfo" id="MesWarehousingInfoResult">
        <result property="id"    column="id"    />
        <result property="crystalCode"    column="crystal_code"    />
        <result property="spec"    column="spec"    />
        <result property="type"    column="type"    />
        <result property="length"    column="length"    />
        <result property="thicknessSplice"    column="thickness_splice"    />
