            <if test="connectibleCount1 != null">connectible_count1 = #{connectibleCount1},</if>
            <if test="connectionType1 != null">connection_type1 = #{connectionType1},</if>
            <if test="slicerModel1 != null">slicer_model1 = #{slicerModel1},</if>
            <if test="workingTime1 != null">working_time1 = #{workingTime1},</if>
            <if test="transmittingState1 != null">transmitting_state1 = #{transmittingState1},</if>
            <if test="storageState1 != null">storage_state1 = #{storageState1},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteMesCrystalCheckById" parameterType="Long">
        delete from mes_crystal_check where id = #{id}
    </delete>

    <delete id="deleteMesCrystalCheckByIds" parameterType="String">
        delete from mes_crystal_check where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>