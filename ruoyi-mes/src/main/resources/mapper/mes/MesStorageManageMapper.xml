<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.mes.mapper.MesStorageManageMapper">
    
    <resultMap type="MesStorageManage" id="MesStorageManageResult">
        <result property="id"    column="id"    />
        <result property="code"    column="code"    />
        <result property="state"    column="state"    />
        <result property="createdTime"    column="created_time"    />
        <result property="creater"    column="creater"    />
        <result property="revisedTime"    column="revised_time"    />
        <result property="reviser"    column="reviser"    />
    </resultMap>

    <sql id="selectMesStorageManageVo">
        select id, code, state, created_time, creater, revised_time, reviser from mes_storage_manage
    </sql>

    <select id="selectMesStorageManageList" parameterType="MesStorageManage" resultMap="MesStorageManageResult">
        <include refid="selectMesStorageManageVo"/>
        <where>  
            <if test="code != null  and code != ''"> and code = #{code}</if>
            <if test="state != null  and state != ''"> and state = #{state}</if>
        </where>
    </select>
    
    <select id="selectMesStorageManageById" parameterType="Long" resultMap="MesStorageManageResult">
        <include refid="selectMesStorageManageVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertMesStorageManage" parameterType="MesStorageManage" useGeneratedKeys="true" keyProperty="id">
        insert into mes_storage_manage
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="code != null">code,</if>
            <if test="state != null">state,</if>
            <if test="createdTime != null">created_time,</if>
            <if test="creater != null">creater,</if>
            <if test="revisedTime != null">revised_time,</if>
            <if test="reviser != null">reviser,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="code != null">#{code},</if>
            <if test="state != null">#{state},</if>
            <if test="createdTime != null">#{createdTime},</if>
            <if test="creater != null">#{creater},</if>
            <if test="revisedTime != null">#{revisedTime},</if>
            <if test="reviser != null">#{reviser},</if>
         </trim>
    </insert>

    <update id="updateMesStorageManage" parameterType="MesStorageManage">
        update mes_storage_manage
        <trim prefix="SET" suffixOverrides=",">
            <if test="code != null">code = #{code},</if>
            <if test="state != null">state = #{state},</if>
            <if test="createdTime != null">created_time = #{createdTime},</if>
            <if test="creater != null">creater = #{creater},</if>
            <if test="revisedTime != null">revised_time = #{revisedTime},</if>
            <if test="reviser != null">reviser = #{reviser},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteMesStorageManageById" parameterType="Long">
        delete from mes_storage_manage where id = #{id}
    </delete>

    <delete id="deleteMesStorageManageByIds" parameterType="String">
        delete from mes_storage_manage where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>